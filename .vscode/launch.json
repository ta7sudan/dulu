	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug ls",
			"program": "${workspaceFolder}/bin/index.js",
			"args": [
				"ls",
				"multicmd-cli"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug create",
			"program": "${workspaceFolder}/bin/index.js",
			"args": [
				"create",
				"multicmd-cli",
				"myproject",
				"-d",
				"c:/codes/demo"
			]
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach Process",
			"processId": "${command:PickProcess}"
		}
	]
}