{
  "name": "dulu",
  "version": "0.1.0",
  "description": "Scaffolding tool for my development",
  "main": "src/index.js",
  "bin": {
    "dulu": "./bin/index.js"
  },
  "man": "./man/doc.1",
  "engines": {
    "node": ">=10.9.0"
  },
  "scripts": {
    "lint": "eslint src bin test",
    "format": "prettier --config .prettierrc.js --write src/**/*.js bin/**/*.js",
    "inspect": "jsinspect src bin",
    "test": "echo todo",
    "prepare": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ta7sudan/dulu.git"
  },
  "keywords": [
    "scaffolding",
    "cli"
  ],
  "author": "ta7sudan",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ta7sudan/dulu/issues"
  },
  "homepage": "https://github.com/ta7sudan/dulu#readme",
  "files": [
    "/bin",
    "/src",
    "/man",
    "/docs",
    "/templates"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --config .prettierrc.js --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.0-rc.13",
    "jsinspect": "^0.12.7",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "art-template": "^4.13.0",
    "chalk": "^2.4.1",
    "cli-table": "^0.3.1",
    "download-git-repo": "^1.1.0",
    "figlet": "^1.2.0",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.2.0",
    "ora": "^3.0.0",
    "semver": "^5.5.1",
    "signale": "^1.2.1",
    "yargonaut": "^1.1.3",
    "yargs": "^12.0.0"
  }
}
